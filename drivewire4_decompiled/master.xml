<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<master xmlns="http://groupunix.org/DriveWireConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://groupunix.org/DriveWireConfig Master.xsd ">

<drivewire-config>

<UIEnabled category="advanced" type="boolean">true</UIEnabled>
<UIPort category="advanced" max="65535" min="1" type="int">6800</UIPort>
<UseRXTX category="advanced, device" type="boolean">true</UseRXTX>
<LoadRXTX category="advanced, device" type="boolean">true</LoadRXTX>
<LoadRXTXPath category="advanced, device" type="directory"/>
<ConfigAutosave category="advanced" type="boolean">true</ConfigAutosave>
<LogLevel category="server, logging" list="ALL,DEBUG,INFO,WARN,ERROR,FATAL" type="list">INFO</LogLevel>
<LogToConsole category="logging" type="boolean">false</LogToConsole>
<LogFormat category="advanced, logging" type="string"/>
<LogToFile category="logging" type="boolean">false</LogToFile>
<LogFile category="logging" type="file"/>
<LogUIConnections category="logging,advanced" type="boolean">false</LogUIConnections>
<DiskLazyWriteInterval category="advanced,disk" max="60000" min="1000" type="int">5000</DiskLazyWriteInterval>
<LocalDiskDir category="advanced,disk" type="directory"/>
<StatusInterval category="advanced, server" max="60000" min="1000" type="int">1000</StatusInterval>
<instance category="instance">

		<CurrentPrinter category="printing" section="instance.Printer" type="section">Text</CurrentPrinter>

		<Printer category="printing">
			<Driver list="TEXT,FX80" type="list">FX80</Driver>
			<OutputDir type="directory"/>
			<CharacterFile type="file">default.chars</CharacterFile>
			<Columns max="132" min="1" type="int">80</Columns>
			<Lines max="132" min="1" type="int">66</Lines>
			<DPI max="1200" min="50" type="int">300</DPI>
			<ImageFormat list="JPG,GIF,PNG,BMP" type="list">PNG</ImageFormat>
			<FlushCommand type="string"/>
			<OutputFile type="file"/>
		</Printer>

		

		<UseMIDI category="midi" type="boolean">false</UseMIDI>
		<MIDISynthDefaultSoundbank category="midi" type="file"/>
		<MIDISynthDefaultProfile category="midi" section="midisynthprofile" type="section">gm</MIDISynthDefaultProfile>
        <MIDIDefaultOutput category="midi"/>
        
		<HelpFile category="advanced">help.xml</HelpFile>

		<LogOpCodePolls category="logging,advanced" type="boolean">false</LogOpCodePolls>
		<LogOpCode category="logging,advanced" type="boolean">false</LogOpCode>
		<LogDeviceBytes category="logging,advanced" type="boolean">false</LogDeviceBytes>
		<LogMIDIBytes category="logging,advanced" type="boolean">false</LogMIDIBytes>
		<LogVPortBytes category="logging,advanced" type="boolean">false</LogVPortBytes>
        <LogTiming category="logging,advanced" type="boolean">false</LogTiming>

		<DeviceType category="device" list="serial,tcp-server,tcp-client,dummy" type="list">dummy</DeviceType>
		<TCPServerPort category="device" max="65535" min="1" type="int">65504</TCPServerPort>
		<TCPClientPort category="device" max="65535" min="1" type="int"/>
		<TCPClientHost category="device" type="string"/>
		<SerialDevice category="device" type="serialdev"/>
		<DetectDATurbo category="advanced, device" type="boolean">false</DetectDATurbo>
		<SerialRate category="device" list="38400,57600,115200,230400,460800,921600" type="list">115200</SerialRate>
		<SerialParity category="device, advanced" list="none,even,odd,mark,space" type="list">none</SerialParity>
		<SerialStopbits category="device, advanced" list="1,1.5,2" type="list">1</SerialStopbits>
		<DW3Only category="device,advanced" type="boolean">false</DW3Only>
		<ProtocolFlipOutputBits category="advanced, device" type="boolean">false</ProtocolFlipOutputBits>
		<ProtocolResponsePrefix category="advanced, device" type="boolean">false</ProtocolResponsePrefix>
		<ProtocolDisableReadChecksum category="advanced, device" type="boolean">false</ProtocolDisableReadChecksum>
		<ProtocolLieAboutCRC category="advanced, device" type="boolean">false</ProtocolLieAboutCRC>
		<DeviceFailRetryTime category="advanced, device" max="60000" min="1000" type="int">6000</DeviceFailRetryTime>
		<DiskSectorSize category="advanced, disk" max="16384" min="1" type="int">256</DiskSectorSize>
		<ReadDelay category="advanced, disk" max="16384" min="1" type="int"/>
		<OpTimeSendsDOW category="advanced, device" type="boolean">false</OpTimeSendsDOW>
		<DWCommandOutputWidth category="advanced, device" max="240" min="1" type="int">80</DWCommandOutputWidth>

		<ListenAddress category="networking,advanced" type="networkinterface"/>
		<TermPort category="networking" max="65535" min="1" type="int"/>
		<TelnetBannerFile category="networking" type="file"/>
		<TelnetNoPortsBannerFile category="networking,advanced" type="file"/>


		<HDBDOSMode category="disk,advanced" type="boolean">false</HDBDOSMode>
