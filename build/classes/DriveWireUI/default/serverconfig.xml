<instance>
    		<Name>Default</Name>
			
			<CurrentPrinter>text</CurrentPrinter>
            
			<Printer>
			 <Name>FX80</Name>
			 <Driver>FX80</Driver>
			 <OutputDir>cocoprints</OutputDir>
			 <CharacterFile>default.chars</CharacterFile>
			 <Columns>80</Columns>
			 <Lines>66</Lines>
			 <DPI>300</DPI>
			 <ImageFormat>PNG</ImageFormat>
			</Printer>
			<Printer>
			 <Name>text</Name>
			 <Driver>TEXT</Driver>
			 <OutputDir>cocoprints</OutputDir>
			</Printer>
			
			
